{
  "name": "qappio-monorepo",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "i": "pnpm install -r",
    "build": "pnpm -r build",
    "dev": "concurrently -k -n ADMIN,MOBILE -c green,cyan \"pnpm --filter qappio-admin dev\" \"pnpm --filter qappio-mobile start\"",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.3.3"
  }
}
