{
  "name": "qappio-monorepo",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "i": "pnpm install -r",
    "build": "pnpm -r build",
    "dev": "concurrently -k -n ADMIN,MOBILE -c green,cyan \"pnpm --filter qappio-admin dev\" \"pnpm --filter qappio-mobile start\"",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.3.3"
  }
}
{
  "scripts": {
    "db:migrate": "ts-node packages/db/runMigrations.ts",
    "admin:dev": "cd qappion/admin && npm run dev",
    "admin:build": "cd qappion/admin && npm run build",
    "admin:start": "cd qappion/admin && npm run start"
  },
  "devDependencies": {
    "@types/node": "24.3.0",
    "@types/pg": "^8.10.9",
    "@types/react": "19.1.12",
    "pg": "^8.16.3",
    "ts-node": "^10.9.2",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.2",
    "dotenv": "^17.2.1",
    "next": "^15.5.2"
  }
}
