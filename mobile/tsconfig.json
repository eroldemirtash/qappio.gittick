{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "noEmit": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "nativewind",
      "expo-router"
    ]
  },
  "include": [
    "expo-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "expo-router/entry",
    "nativewind-env.d.ts",
    ".expo/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "test53"
  ]
}
